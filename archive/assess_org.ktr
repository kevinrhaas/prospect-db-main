    <sql>select company_conform, assessed_company, string_agg(conform_id::TEXT, ',') conform_id_list, count(*) cnt from
(
select cc.conform_id ,
cc.company_conform , cc.company_clean,  cc.is_company ,  ai_simil_company_name, 
case when is_company = 'N' then ''  
else case when ai_simil_company_name = 'not standardized' and is_company = 'Y' then company_conform else ai_simil_company_name end end assessed_company,  
cc.dist_dl, cc.dist_nw, cc.simil_jaro, cc.simil_jarowink, certainty_score 
from marketing.clean_company cc
order by assessed_company
) a
where assessed_company &lt;&gt; '' and is_company= 'Y'
group by 1, 2
order by 4 desc
</sql>
