        <jsScript_script>// Function to standardize company names
function standardizeCompanyName(name) {
  if (name == null) return null;

  // Trim leading and trailing whitespace
  var trimmed = name.trim();

  // Remove unnecessary punctuation (commas, periods, etc.) and extra spaces
  trimmed = trimmed.replace(/[.,\/#!$%\^&amp;\*;:{}=\-_`~()]+/g, '').replace(/\s+/g, ' ');

  // Convert to Title Case
  var words = trimmed.toLowerCase().split(' ');
  for (var i = 0; i &lt; words.length; i++) {
    if (words[i].length > 0) { // Ensure the word is not empty
      words[i] = words[i].charAt(0).toUpperCase() + words[i].slice(1);
    }
  }
  trimmed = words.join(' ');

  return trimmed;
}


</jsScript_script>
