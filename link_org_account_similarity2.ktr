    <connection>MARKETING</connection>
    <sql>    SELECT 
        do1.id AS dim_organization_id, 
        a."Account_ID", 
        a."Account_Name", 
		do1.organization_standard,
        similarity(do1.organization_standard, a."Account_Name") AS similarity_score,
        ROW_NUMBER() OVER (PARTITION BY do1.id ORDER BY similarity(do1.organization_standard, a."Account_Name") DESC) AS rank
    FROM ${SCHEMA}.dim_organization do1
    JOIN ${SCHEMA}.raw_pentaho_salesforce_account a 
        ON a."Parent_Account_ID" IS null and is_organization = 'Y' and do1.id &lt;&gt; 0
    WHERE similarity(do1.organization_standard, a."Account_Name") > 0.15</sql>
